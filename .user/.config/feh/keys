## ~/.config/feh/keys ::

## unset ::
action_0
quit
save_filelist
save_image
size_to_image
toggle_filenames
toggle_fixed_geometry
toggle_fullscreen
toggle_info
toggle_filenames
toggle_actions
toggle_pointer
save_image
prev_dir
next_dir

## mappings ::
close q

# open xterm window with env var img=<current image>:
action_7 C-semicolon

# close every feh window:
action_6 Q

# open in imv:
action_2 backslash

# fullscreen (via i3-msg):
action_1 f

# toggle action defs overlay:
toggle_actions F

# toggle exif info overlay:
toggle_exif E

# toggle status bar:
toggle_info S

# print info in terminal:
action_3 question

# next/prev image:
next_img n
prev_img p

# next/prev 5% of file list:
jump_fwd N Next
jump_back P Prior

# next/prev every feh window:
action_4 bracketright
action_5 bracketleft

# first/last:
jump_first g
jump_last G

# scroll:
scroll_left h
scroll_left_page H
scroll_down j
scroll_down_page J
scroll_up k
scroll_up_page K
scroll_right l
scroll_right_page L

# remove from file list:
remove x

# delete and remove from file list:
delete C-x Delete

# load random image from file list:
jump_random m

# reload:
reload_image C-l

render Return

# resize window to image:
size_to_image W

# zoom: fill window:
zoom_fill w

# zoom: 100%:
zoom_default e 0

# zoom: fit to window:
zoom_fit r

# zoom in/out:
zoom_in i
zoom_out o

# toggle
toggle_aliasing A

# toggle keeping zoom level between images:
toggle_keep_vp V

# pause slideshow:
toggle_pause space

# vim: set ft=config:
